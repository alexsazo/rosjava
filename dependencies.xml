<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project name="dependencies" default="depend.all">
  <dirname property="dependencies.basedir" file="${ant.file.dependencies}" />

  <target name="depend.all" depends="depend.java, depend.android" />

  <target name="depend.ws-commons-util">
    <ant dir="${dependencies.basedir}/rosjava/ws-commons-util-1.0.2-src" inheritAll="false" />
  </target>

  <target name="depend.apache-xmlrpc" depends="depend.ws-commons-util">
    <ant dir="${dependencies.basedir}/rosjava/apache-xmlrpc-3.1.3-src" inheritAll="false" />
  </target>

  <target name="depend.java" depends="depend.apache-xmlrpc">
    <ant dir="${dependencies.basedir}/rosjava" inheritAll="false" />
  </target>

  <target name="depend.android" depends="depend.java">
    <ant dir="${dependencies.basedir}/android" inheritAll="false" />
  </target>
</project>
